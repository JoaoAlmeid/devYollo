#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@sentry/profiling-node/scripts/prune-profiler-binaries.mjs" "$@"
  ret=$?
else 
  node  "$basedir/../@sentry/profiling-node/scripts/prune-profiler-binaries.mjs" "$@"
  ret=$?
fi
exit $ret
